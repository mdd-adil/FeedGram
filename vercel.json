{
  "version": 2,
  "buildCommand": "npm --prefix client install && npm --prefix client run build",
  "outputDirectory": "client/build",
  "installCommand": "npm --prefix client install",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
